<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.asuper.abocha.cs_go">

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Map.MapActivity">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MapDetail.MapDetailActivity">
        </activity>
        <activity
            android:name=".GalleryBigDetail.GalleryBigDetail"
            android:theme="@style/Transparent">
        </activity>
        <activity android:name=".VoiceRecordActivity">
        </activity>
        <activity android:name=".Stream.StreamActivity">
        </activity>
    </application>

</manifest>